{
  "name": "travel-app",
  "version": "1.0.0",
  "description": "a mini travel app using webpack",
  "main": "main.js",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "scripts": {
    "test": "jest --watch --runInBand --detectOpenHandles --testTimeout=20000",
    "start": "npm-run-all --parallel build-prod server",
    "serve": "npm-run-all --parallel build-dev server",
    "server": "node src/server/start.js",
    "http": "http-server dist",
    "build-prod": "webpack --config webpack.prod.js",
    "build-dev": "webpack-dev-server --config webpack.dev.js --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akramgassem/travell-app.git"
  },
  "keywords": [
    "webpack",
    "travel",
    "geonames",
    "darksky",
    "pixabay",
    "countries-api",
    "api"
  ],
  "author": "@akramg",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "breakpoints-js": "^1.0.6",
    "bulma": "^0.8.2",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "nodemon": "^2.0.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "extract-loader": "^4.0.3",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.3",
    "jest": "^25.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "bugs": {
    "url": "https://github.com/akramgassem/travell-app/issues"
  },
  "homepage": "https://github.com/akramgassem/travell-app#readme"
}
